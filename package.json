{
  "name": "jstdd",
  "version": "1.0.0",
  "description": "Curso JSTDD",
  "main": ".eslintrc.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint && npm run test:coverage",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --watch",
    "test:coverage": "nyc npm test"
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jean-Pierre-97s/CursoJsTdd.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Jean Pierre <Jean_Pierre_97@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jean-Pierre-97s/CursoJsTdd/issues"
  },
  "homepage": "https://github.com/Jean-Pierre-97s/CursoJsTdd#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "chai": "^4.3.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^5.0.9",
    "mocha": "^8.3.0",
    "nyc": "^15.1.0",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {}
}
